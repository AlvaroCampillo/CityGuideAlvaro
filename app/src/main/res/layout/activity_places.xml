<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:backgroundTint="#EDEBF4"
    tools:context=".Places">

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="146dp"
        android:layout_height="69dp"
        android:layout_marginEnd="80dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView12"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView14"
        app:layout_constraintVertical_bias="0.536"
        app:srcCompat="@drawable/cala" />

    <Button
        android:id="@+id/wonder"
        android:layout_width="179dp"
        android:layout_height="71dp"
        android:layout_marginTop="42dp"
        android:background="#101510"
        android:backgroundTint="#00FFFFFF"
        android:text="Santa María del Naranco"
        android:textAllCaps="false"
        android:textColor="#EADEDE"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gascona"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/tours"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Places"
        android:textAlignment="center"
        android:textColor="#0C0808"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.043" />

    <Button
        android:id="@+id/llagar"
        android:layout_width="182dp"
        android:layout_height="66dp"
        android:layout_marginStart="4dp"
        android:background="#0B0C0A"
        android:text="Calatrava"
        android:textAllCaps="false"
        android:textColor="#EAE2E2"
        android:textSize="18sp"
        android:textStyle="bold"
        app:iconTint="#00FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/fontan"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView14"
        app:layout_constraintVertical_bias="0.56" />

    <Button
        android:id="@+id/fontan"
        android:layout_width="177dp"
        android:layout_height="61dp"
        android:layout_marginBottom="32dp"
        android:background="#010202"
        android:backgroundTint="#003A2626"
        android:text="El Fontán"
        android:textAllCaps="false"
        android:textColor="#F8EAEA"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/imageView10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.027"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/catedral"
        android:layout_width="188dp"
        android:layout_height="57dp"
        android:layout_marginTop="84dp"
        android:background="@drawable/catedral"
        android:backgroundTint="#000000"
        android:text="Cathedral"
        android:textAllCaps="false"
        android:textColor="#F1EAEA"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/gascona"
        android:layout_width="178dp"
        android:layout_height="61dp"
        android:layout_marginTop="36dp"
        android:background="#000000"
        android:backgroundTint="#00392525"
        android:text="Gascona"
        android:textAllCaps="false"
        android:textColor="#E1D0D0"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.026"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fontan" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="111dp"
        android:layout_height="100dp"
        android:layout_marginBottom="48dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/wonder"
        app:srcCompat="@drawable/stamariap" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="123dp"
        android:layout_height="72dp"
        android:layout_marginBottom="20dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView8"
        app:layout_constraintStart_toEndOf="@+id/gascona"
        app:srcCompat="@drawable/gasco" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="122dp"
        android:layout_height="61dp"
        android:layout_marginBottom="32dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView10"
        app:layout_constraintStart_toEndOf="@+id/fontan"
        app:srcCompat="@drawable/fontan" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="91dp"
        android:layout_height="63dp"
        android:layout_marginStart="184dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView16"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tours"
        app:srcCompat="@drawable/catedral" />

</androidx.constraintlayout.widget.ConstraintLayout>